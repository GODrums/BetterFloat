{
	"name": "betterfloat",
	"displayName": "BetterFloat",
	"version": "3.2.2",
	"description": "Enhance your website experience on 9+ CS2 skin markets!",
	"author": "Rums",
	"license": "CC BY NC SA 4.0",
	"homepage": "https://betterfloat.com",
	"scripts": {
		"preinstall": "lefthook install",
		"dev": "plasmo dev",
		"dev:firefox": "plasmo dev --target=firefox-mv3",
		"build": "plasmo build",
		"build:firefox": "plasmo build --target=firefox-mv3",
		"package": "plasmo package",
		"lint": "biome ci src/",
		"format": "biome format src/ --write && biome check src/ --fix"
	},
	"dependencies": {
		"@plasmohq/messaging": "^0.7.1",
		"@plasmohq/storage": "^1.15.0",
		"@radix-ui/react-accordion": "^1.2.11",
		"@radix-ui/react-avatar": "^1.1.10",
		"@radix-ui/react-checkbox": "^1.3.2",
		"@radix-ui/react-dialog": "^1.1.14",
		"@radix-ui/react-icons": "^1.3.2",
		"@radix-ui/react-label": "^2.1.7",
		"@radix-ui/react-popover": "^1.1.14",
		"@radix-ui/react-scroll-area": "^1.2.9",
		"@radix-ui/react-select": "^2.2.5",
		"@radix-ui/react-separator": "^1.1.7",
		"@radix-ui/react-slider": "^1.3.5",
		"@radix-ui/react-slot": "^1.2.3",
		"@radix-ui/react-switch": "^1.2.5",
		"@radix-ui/react-tabs": "^1.1.12",
		"@radix-ui/react-toast": "^1.2.14",
		"@radix-ui/react-tooltip": "^1.2.7",
		"@tsparticles/engine": "^3.8.1",
		"@tsparticles/react": "^3.0.0",
		"@tsparticles/slim": "^3.8.1",
		"@types/common-tags": "^1.8.4",
		"@webext-core/isolated-element": "^1.1.2",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"common-tags": "^1.8.2",
		"framer-motion": "^11.18.2",
		"fuse.js": "^7.1.0",
		"jose": "^5.10.0",
		"lucide-react": "^0.469.0",
		"plasmo": "0.90.5",
		"react": "18.3.1",
		"react-colorful": "^5.6.1",
		"react-dom": "18.3.1",
		"simplex-noise": "^4.0.3",
		"socket.io-client": "^4.8.1",
		"socket.io-msgpack-parser": "^3.0.2",
		"tailwind-merge": "^2.6.0",
		"tailwindcss-animate": "^1.0.7",
		"zod": "^3.25.64"
	},
	"devDependencies": {
		"@biomejs/biome": "2.0.0",
		"@parcel/packager-ts": "2.9.3",
		"@types/chrome": "0.0.315",
		"@types/node": "22.10.2",
		"@types/react": "18.3.11",
		"@types/react-dom": "18.3.1",
		"cs-tierlist": "^1.2.2",
		"csgo-fade-percentage-calculator": "^1.1.6",
		"currency-symbol-map": "^5.1.0",
		"decimal.js": "^10.5.0",
		"lefthook": "^1.11.13",
		"postcss": "8.4.49",
		"resq": "^1.11.0",
		"tailwindcss": "3.4.17",
		"typescript": "5.7.2"
	},
	"manifest": {
		"key": "$CRX_PUBLIC_KEY",
		"browser_specific_settings": {
			"gecko": {
				"id": "$FIREFOX_EXT_ID",
				"strict_min_version": "128.0"
			}
		},
		"optional_host_permissions": [
			"https://*/*",
			"http://*/*"
		],
		"externally_connectable": {
			"matches": [
				"https://*.rums.dev/*"
			]
		},
		"permissions": [
			"unlimitedStorage",
			"storage",
			"scripting",
			"activeTab",
			"omnibox",
			"notifications"
		],
		"host_permissions": [
			"*://*.csfloat.com/*",
			"*://*.skinport.com/*",
			"*://*.skinbid.com/*",
			"*://*.avan.market/*",
			"*://*.bitskins.com/*",
			"*://*.cs.money/*",
			"*://*.dmarket.com/*",
			"*://*.steamcommunity.com/*",
			"*://*.steampowered.com/*",
			"*://*.gamerpay.gg/*",
			"*://*.haloskins.com/*",
			"*://*.lis-skins.com/*",
			"*://*.market.csgo.com/*",
			"*://*.rapidskins.com/*",
			"*://*.shadowpay.com/*",
			"*://*.skinbaron.de/*",
			"*://*.skinflow.gg/*",
			"*://*.skinout.com/*",
			"*://*.skin.place/*",
			"*://*.skinsmonkey.com/*",
			"*://*.skinswap.com/*",
			"*://*.swap.gg/*",
			"*://*.tradeit.gg/*",
			"*://*.waxpeer.com/*",
			"*://*.whitemarket.com/*",
			"*://*.youpin898.com/*"
		],
		"omnibox": {
			"keyword": "bf"
		}
	},
	"pnpm": {
		"peerDependencyRules": {
			"allowedVersions": {
				"svgo": "2.8.0"
			}
		},
		"onlyBuiltDependencies": [
			"@swc/core",
			"lefthook"
		]
	}
}